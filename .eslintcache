t MySnackbar = () => {\n  const [snackPack, setSnackPack] = React.useState([]);\n  const [alertType, setAlertType] = React.useState(\"success\");\n  const [open, setOpen] = React.useState(false);\n  const [messageInfo, setMessageInfo] = React.useState(undefined);\n\n  const totalItems = useSelector((state) => state.cart.total_items);\n  const errorsCount = useSelector((state) => state.errorsCount);\n\n  const prevItems = useRef(totalItems);\n  const prevErrors = useRef();\n\n  const showSnackbar = (message) => {\n    setSnackPack((prev) => [...prev, { message, key: new Date().getTime() }]);\n  };\n\n  useEffect(() => {\n    // if (totalItems && !prevItems.current) prevItems.current = totalItems;\n    if (errorsCount && !prevErrors.current) prevErrors.current = errorsCount;\n  }, []);\n\n  // watch changes in cart total items\n  useEffect(() => {\n    if (\n      (totalItems &&\n        prevItems.current !== totalItems &&\n        totalItems !== \"initial\" &&\n        prevItems.current !== \"initial\") ||\n      (!totalItems && prevItems.current && prevItems.current !== \"initial\")\n    ) {\n      // show success\n      prevItems.current = totalItems;\n      setAlertType(\"success\");\n      showSnackbar(\"SHPORTA U PËRDITËSUA\");\n    }\n\n    if (prevItems.current === \"initial\") prevItems.current = totalItems;\n  }, [totalItems]);\n\n  // watch changes in error counter\n  useEffect(() => {\n    if (\n      (errorsCount && prevErrors.current !== errorsCount) ||\n      (!errorsCount && prevErrors.current)\n    ) {\n      // show error\n      setAlertType(\"error\");\n      showSnackbar(\"PATI NJË PROBLEM\");\n    }\n  }, [errorsCount]);\n\n  useEffect(() => {\n    if (snackPack.length && !messageInfo) {\n      // Set a new snack when we don't have an active one\n      setMessageInfo({ ...snackPack[0] });\n      setSnackPack((prev) => prev.slice(1));\n      setOpen(true);\n    } else if (snackPack.length && messageInfo && open) {\n      // Close an active snack when a new one is added\n      setOpen(false);\n    }\n  }, [snackPack, messageInfo, open]);\n\n  const handleClose = (event, reason) => {\n    if (reason === \"clickaway\") {\n      return;\n    }\n    setOpen(false);\n  };\n\n  const handleExited = () => {\n    setMessageInfo(undefined);\n  };\n\n  return (\n    <Snackbar\n      key={messageInfo ? messageInfo.key : undefined}\n      anchorOrigin={{\n        vertical: \"bottom\",\n        horizontal: \"right\",\n      }}\n      TransitionComponent={slide}\n      open={open}\n      autoHideDuration={4000}\n      onClose={handleClose}\n      onExited={handleExited}\n    >\n      <Alert\n        severity={alertType}\n        variant=\"filled\"\n        style={{ boxShadow: \"0px 5px 10px rgba(0,0,0,0.5)\" }}\n      >\n        {messageInfo && messageInfo.message}\n      </Alert>\n    </Snackbar>\n  );\n};\n\nexport default MySnackbar;\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Products/ProductDrawer/ProductDrawer.jsx",["411","412"],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Products/ProductDrawer/styles.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Products/ProductDrawer/ImageCarousel/ImageCarousel.jsx",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Products/ProductDrawer/ImageCarousel/styles.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/store/reducers/backdrop/reducer.js",["413"],"import * as C from \"../../constant\";\n\nconst initialState = false;\n\nexport default (state = initialState, { type, payload }) => {\n  switch (type) {\n    case C.SET_BACKDROP:\n      return payload;\n\n    default:\n      return state;\n  }\n};\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Footer/Footer.jsx",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Footer/style.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Support/MyOrders/MyOrders.jsx",["414","415"],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Support/MyOrders/styles.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Support/CustomerOrders/CustomerOrders.jsx",["416"],"import { Button, Container, Paper, Typography } from \"@material-ui/core\";\nimport Hero from \"../../Hero/Hero\";\nimport React, { useEffect, useRef } from \"react\";\nimport useStyles from \"./styles\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { UserServices } from \"../../../services\";\nimport { generalActions } from \"../../../store/actions\";\nimport { Link } from \"react-router-dom\";\nimport OrdersAccordion from \"./OrdersAccordion/OrdersAccordion\";\n\nconst CustomerOrders = () => {\n  const classes = useStyles();\n  const customerId = useSelector((state) => state.customer.id);\n  const orders = useSelector((state) => state.customer.orders);\n  const dispatch = useDispatch();\n\n  const prevCustomerId = useRef();\n\n  useEffect(() => {\n    document.title = \"Pazari - Blerjet e mia\";\n\n    // call api for customer orders\n    const getOrders = async () => {\n      try {\n        const res = await UserServices.listOrdersForCustomer(customerId);\n        if (res.status === 200) {\n          dispatch(generalActions.setCustomerOrders(res.data));\n          prevCustomerId.current = customerId;\n        }\n        dispatch(generalActions.setBackdrop(false));\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    if (customerId && customerId !== prevCustomerId.current) getOrders();\n  }, [customerId]);\n\n  const logout = () => {\n    console.log(\"loggingout\");\n    // delete user from localStorage\n    localStorage.removeItem(\"user\");\n\n    // delete customer/user from redux\n    dispatch(generalActions.setCustomerId(\"\"));\n    dispatch(generalActions.setCustomerOrders({}));\n  };\n\n  const NoOrders = () => (\n    <Paper variant=\"outlined\" className={classes.noOrdersPaper}>\n      <Typography>Ju nuk keni asnjë porosi.</Typography>\n      <Button\n        component={Link}\n 