roductInDrawerAndOpenDrawer = () => {\n    dispatch(productActions.setProductInDrawer(product));\n    dispatch(productActions.openDrawer());\n  };\n\n  return (\n    <div>\n      <Card className={classes.root}>\n        <CardActionArea onClick={setProductInDrawerAndOpenDrawer}>\n          <CardMedia\n            className={classes.media}\n            image={\n              product.media.type === \"image\" && product.media.source\n                ? product.media.source\n                : noImageUrl\n            }\n            title={product.name}\n          />\n          <CardContent>\n            <Typography\n              className={classes.productName}\n              gutterBottom\n              variant=\"h6\"\n              component=\"h6\"\n            >\n              {product.name}\n            </Typography>\n            <Typography variant=\"body2\" color=\"textSecondary\" component=\"p\">\n              {product.price.formatted_with_code}\n            </Typography>\n          </CardContent>\n        </CardActionArea>\n        <CardActions>\n          <Button\n            startIcon={<AddShoppingCart />}\n            color=\"primary\"\n            aria-label=\"upload picture\"\n            component=\"span\"\n            disabled={disabled}\n            onClick={() => {\n              setDisabled(true);\n              dispatch(cartActions.addProductToCartAsync(product.id))\n                .then(() => setDisabled(false))\n                .catch((error) => {\n                  console.log(error);\n                  setDisabled(false);\n                  dispatch(generalActions.changeErrorsCount());\n                });\n            }}\n          >\n            {disabled ? \"Prisni...\" : \"Shto në shportë\"}\n          </Button>\n        </CardActions>\n      </Card>\n    </div>\n  );\n};\n\nexport default Product;\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Navbar/Navbar.jsx",["310"],"import React from \"react\";\nimport { NavLink, useLocation 