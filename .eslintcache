m</div>;\n\n  const next = () => setActiveStep((step) => step + 1);\n  const back = () => setActiveStep((step) => step - 1);\n\n  const Form = () =>\n    activeStep === 0 ? (\n      <AddressForm checkoutId={checkoutToken.id} next={next} />\n    ) : (\n      <PaymentForm back={back} />\n    );\n\n  return (\n    <>\n      {loadingCheckout ? (\n        <div className={classes.loader}>\n          <CircularProgress />\n        </div>\n      ) : null}\n      <main className={classes.layout}>\n        <Grid container>\n          <Grid item align=\"right\" xs={12}>\n            <Tooltip title=\"Kthehu tek shporta\" arrow placement=\"left\">\n              <IconButton\n                component={Link}\n                to=\"/cart\"\n                color=\"primary\"\n                aria-label=\"anulo\"\n              >\n                <Close />\n              </IconButton>\n            </Tooltip>\n          </Grid>\n          <Grid item xs={12}>\n            <Paper className={classes.paper}>\n              <Typography variant=\"h4\" align=\"center\">\n                Arka\n              </Typography>\n              <Stepper activeStep={activeStep} className={classes.stepper}>\n                {steps.map((step) => (\n                  <Step key={step}>\n                    <StepLabel>{step}</StepLabel>\n                  </Step>\n                ))}\n              </Stepper>\n              {activeStep === steps.length ? <Confirmation /> : <Form />}\n            </Paper>\n          </Grid>\n        </Grid>\n      </main>\n    </>\n  );\n};\n\nexport default Checkout;\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/store/actions/index.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/store/actions/categories/actions.js",["292"],"import * as C from \"../../constant\";\nimport { commerce } from \"../../../lib/commerce\";\n\n// actions\n\nexport const setCategories = (categories) => ({\n  type: C.SET_CATEGORIES,\n  payload: { categories },\n});\n\n// thunks\n\nexport const fetchCategories = () => {\n  return async (dispatch) => {\n    const { data, meta } = await commerce.categories.list();\n    dispatch(setCategories(data));\n  };\n};\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/store/actions/checkout/actions.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/store/reducers/checkout/reducer.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/store/reducers/products/meta/reducer.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/store/reducers/products/data/reducer.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Checkout/styles.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/store/reducers/checkout/shippingCountries/reducer.js",["293"],"import * as C from \"../../../constant\";\n\nconst initialState = {};\n\nexport default (state = initialState, { type, payload }) => {\n  switch (type) {\n    case C.SET_SHIPPING_COUNTRIES:\n      return { ...payload.shippingCountries };\n\n    default:\n      return state;\n  }\n};\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Checkout/PaymentForm.jsx",["294","295","296"],"import {\n  Button,\n  CircularProgress,\n  Divider,\n  Grid,\n  List,\n  ListItem,\n  ListItemText,\n  TextField,\n  Typography,\n} from \"@material-ui/core\";\nimport React, { useState } from \"react\";\nimport { Controller, useForm, FormProvider } from \"react-hook-form\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { commerce } from \"../../lib/commerce\";\nimport {\n  cartActions,\n  checkoutActions,\n  orderActions,\n} from \"../../store/actions\";\nimport useStyles from \"./styles\";\nimport { useHistory } from \"react-router-dom\";\n\nconst formatter = new Intl.NumberFormat(\"en-GB\", {\n  style: \"currency\",\n  currency: \"ALL\",\n});\n\nconst PaymentForm = ({ back }) => {\n  const history = useHistory();\n  const classes = useStyles();\n  const checkout = useSelector((state) => state.checkout);\n  const dispatch = useDispatch();\n\n  const [loading, setLoading] = useState(false);\n\n  const methods = useForm();\n\n  const [shippingPrice, setShippingPrice] = React.useState({ valid: false });\n\n  const fetchShippingPrice = async () => {\n    const checkoutTokenId = checkout.checkoutToken.id;\n    const shipping_option_id = checkout.formData.shippingOption;\n    const country = checkout.formData.shippingCountry;\n    const region = checkout.formData.shippingSubdivision;\n\n    try {\n      const { valid, price } = await commerce.checkout.checkShippingOption(\n        checkoutTokenId,\n        {\n          shipping_option_id,\n          country,\n          region,\n        }\n      );\n      setShippingPrice({\n        valid,\n        price,\n      });\n    } catch (error) {\n      console.log(error);\n    }\n  };\n\n  React.useEffect(() => {\n    fetchShippingPrice();\n  }, [checkout.checkoutToken.id]);\n\n  const Review = () => {\n    let total =\n      shippingPrice.valid && shippingPrice.price\n        ? shippingPrice.price.raw + checkout.checkoutToken.live.subtotal.raw\n        : checkout.checkoutToken.live.subtotal.raw;\n\n    total = formatter.format(total);\n\n    return (\n      <List\n        dense\n        disablePadding\n        style={{ border: \"1px solid rgba(0,0,0,0.2)\", padding: \"0\" }}\n      >\n        {checkout.checkoutToken.live.line_items.map((product, index) => (\n          <div key={product.id}>\n            <ListItem style={{ padding: \"0 3%\" }}>\n              <ListItemText\n                primary={product.name}\n                secondary={`Sasia: ${product.quantity}`}\n              />\n              <Typography variant=\"body2\">\n                {product.line_total.formatted_with_code}\n              </Typography>\n            </ListItem>\n            {index !== checkout.checkoutToken.live.line_items.length - 1 && (\n              <Divider light />\n            )}\n          </div>\n        ))}\n        <Divider />\n        <ListItem style={{ padding: \"0 3%\", margin: \"2.5% 0\" }}>\n          <ListItemText primary=\"Subtotali\" />\n          <Typography>\n            {checkout.checkoutToken.live.subtotal.formatted_with_code}\n          </Typography>\n        </ListItem>\n        <ListItem style={{ padding: \"0 3%\", margin: \"2.5% 0\" }}>\n          <ListItemText />\n          {shippingPrice.valid && shippingPrice.price ? (\n            <Typography variant=\"caption\">\n              + {shippingPrice.price.formatted_with_code} Dërgesa\n            </Typography>\n          ) : (\n            <Typography variant=\"caption\">Dërgesa Falas</Typography>\n          )}\n        </ListItem>\n        <Divider />\n        <ListItem style={{ padding: \"0 3%\", margin: \"2.5% 0\" }}>\n          <ListItemText\n            primary=\"Totali\"\n            primaryTypographyProps={{ variant: \"h6\" }}\n          />\n          <Typography variant=\"h6\">{total}</Typography>\n        </ListItem>\n      </List>\n    );\n  };\n\n  const onCaptureCheckout = async (id, data) => {\n    setLoading(true);\n    console.log(\"captureCheckout: \", id, data);\n    try {\n      // call commerce.checkout.capture\n      const { customer_reference } = await commerce.checkout.capture(id, data);\n      //// set order state with response data\n      dispatch(orderActions.setOrder(customer_reference));\n      // refresh cart\n      dispatch(cartActions.refreshCartAsync());\n      //// navigate to successfull order page\n      history.push(\"/order-success\");\n      ////// store order in session storage\n    } catch (error) {\n      console.log(error);\n    }\n  };\n\n  const handleCaptureCheckout = async (data) => {\n    dispatch(checkoutActions.setCardDetails(data));\n\n    let items = {};\n    checkout.checkoutToken.live.line_items.map((item) => {\n      items[item.id] = { quantity: item.quantity, variants: {} };\n    });\n\n    const orderData = {\n      line_items: items,\n      customer: {\n        firstname: checkout.formData.firstname,\n        lastname: checkout.formData.lastname,\n        email: checkout.formData.email,\n      },\n      shipping: {\n        name: `${checkout.formData.firstname} ${checkout.formData.lastname}`,\n        street: checkout.formData.address1,\n        town_city: checkout.formData.city,\n        county_state: checkout.formData.shippingSubdivision,\n        postal_zip_code: checkout.formData.zip,\n        country: checkout.formData.shippingCountry,\n      },\n      fulfillment: {\n        shipping_method: checkout.formData.shippingOption,\n      },\n      payment: {\n        gateway: \"test_gateway\",\n        card: {\n          number: data.cardnumber,\n          expiry_month: data.expMonth,\n          expiry_year: data.expYear,\n          cvc: data.cvc,\n          postal_zip_code: checkout.formData.zip,\n        },\n      },\n    };\n    onCaptureCheckout(checkout.checkoutToken.id, orderData);\n  };\n\n  return (\n    <>\n      {loading ? (\n        <div className={classes.loader}>\n          <CircularProgress />\n        </div>\n      ) : null}\n      <form onSubmit={methods.handleSubmit(handleCaptureCheckout)}>\n        <Grid container spacing={3}>\n          <Grid container item xs={12} justify=\"center\">\n            <Grid item xs={12}>\n              <Review />\n            </Grid>\n            <Grid\n              style={{ marginTop: \"1rem\" }}\n              container\n              item\n              spacing={3}\n              xs={12}\n            >\n              <Grid item xs={12}>\n                <Controller\n                  control={methods.control}\n                  as={TextField}\n                  name=\"cardnumber\"\n                  label=\"Numri i kartës\"\n                  required\n                  rules={{\n                    minLength: 14,\n                    maxLength: 16,\n                    pattern: /(^4[0-9]{12}(?:[0-9]{3})?$)|(^(?:5[1-5][0-9]{2}|222[1-9]|22[3-9][0-9]|2[3-6][0-9]{2}|27[01][0-9]|2720)[0-9]{12}$)|(3[47][0-9]{13})|(^3(?:0[0-5]|[68][0-9])[0-9]{11}$