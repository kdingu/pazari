{ display: \"flex\" }}\n        >\n          <ThemeProvider theme={theme}>\n            <TextField\n              label=\"Kërko një produkt\"\n              variant=\"outlined\"\n              InputProps={{\n                type: \"search\",\n                className: classes.input,\n              }}\n              className={classes.textfield}\n              value={searchString}\n              onChange={(e) => {\n                if (noSearchResults) setNoSearchResults(false);\n                setSearchString(e.target.value);\n              }}\n            />\n\n            <Button\n              type=\"submit\"\n              variant=\"contained\"\n              color=\"secondary\"\n              disableElevation\n            >\n              Kërko\n            </Button>\n          </ThemeProvider>\n        </form>\n      </Grid>\n      <Grid item xs={12}>\n        {noSearchResults && (\n          <Typography variant=\"subtitle1\" className={classes.infoText}>\n            Asnjë rezultat. Kërko diçka tjetër.\n          </Typography>\n        )}\n        {filter && (\n          <Typography variant=\"subtitle1\" className={classes.infoText}>\n            Rezultati për: \"{filter}\" &nbsp;&nbsp;&nbsp;\n            <Button\n              color=\"secondary\"\n              variant=\"outlined\"\n              size=\"small\"\n              onClick={cleanSearch}\n            >\n              Fshi filtrin\n            </Button>\n          </Typography>\n        )}\n      </Grid>\n    </Grid>\n  );\n};\n\nexport default Search;\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Hero/Search/styles.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Toolbar/Toolbar.jsx",["362"],"import { Button, Container, Grid, Tooltip } from \"@material-ui/core\";\nimport React, { useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { generalActions, productActions } from \"../../store/actions\";\nimport useStyles from \"./styles\";\n\nconst Toolbar = () => {\n  const classes = useStyles();\n  const dispatch = useDispatch();\n  const categories = useSelector((state) => state.categories);\n\n  const handleFilterClick = (categoryId, categoryName) => {\n    dispatch(generalActions.setBackdrop(true));\n    dispatch(productActions.searchByCategoryId(categoryId)).then(() => {\n      dispatch(productActions.setSearchString(categoryName));\n      dispatch(generalActions.setBackdrop(false));\n    });\n  };\n\n  return (\n    <Container maxWidth={false} className={classes.main}>\n      <Container>\n        <Grid container className={classes.innerGrid}>\n          {categories.map((cat) => (\n            <Grid key={cat.id} item xs={12} md={6} lg={3}>\n              <Tooltip title={`Filtro me kategorinë: \"${cat.name}\"`}>\n                <span>\n                  <Button\n                    onClick={() => handleFilterClick(cat.id, cat.name)}\n                    className={classes.button}\n                  >\n                    {cat.name}\n                  </Button>\n                </span>\n              </Tooltip>\n            </Grid>\n          ))}\n        </Grid>\n      </Container>\n    </Container>\n  );\n};\n\nexport default Toolbar;\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Toolbar/styles.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/store/reducers/errorsCounter/reducer.js",["363"],"import * as C from \"../../constant\";\n\nconst initialState = 0;\n\nexport default (state = initialState, { type, payload }) => {\n  switch (type) {\n    case C.INCREMENT_ERRORS:\n      return state + 1;\n\n    default:\n      return state;\n  }\n};\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/store/actions/general/actions.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Snackbar.jsx",["364"],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Products/ProductDrawer/ProductDrawer.jsx",["365","366","367","368","369","370","371","372"],"import {\n  Button,