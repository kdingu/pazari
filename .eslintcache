   <>\n                  <Grid item xs={12}>\n                    <Divider\n                      variant=\"middle\"\n                      style={{ marginTop: \"36px\", marginBottom: \"36px\" }}\n                    />\n                  </Grid>\n                  <Grid item xs={12}>\n                    <Typography variant=\"h5\">Produkte të ngjashme:</Typography>\n                    <Grid container spacing={1}>\n                      {product.related_products.map((product) => (\n                        <Grid\n                          key={product.id}\n                          item\n                          xs={6}\n                          style={{ marginTop: \"26px\" }}\n                        >\n                          <Product\n                            product={product}\n                            handleClick={() => setProductInDrawer(product.id)}\n                          />\n                        </Grid>\n                      ))}\n                    </Grid>\n                  </Grid>\n                </>\n              ) : null}\n            </Grid>\n          </Grid>\n        </form>\n      </div>\n    </Drawer>\n  );\n};\n\nexport default ProductDrawer;\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Products/ProductDrawer/styles.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Products/ProductDrawer/ImageCarousel/ImageCarousel.jsx",["373"],"/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Products/ProductDrawer/ImageCarousel/styles.js",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/store/reducers/backdrop/reducer.js",["374"],"import * as C from \"../../constant\";\n\nconst initialState = false;\n\nexport default (state = initialState, { type, payload }) => {\n  switch (type) {\n    case C.SET_BACKDROP:\n      return payload;\n\n    default:\n      return state;\n  }\n};\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Footer/Footer.jsx",[],"/Users/kdingu/Desktop/Pazari-React/pazari/src/co