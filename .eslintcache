ms = totalItems;\n    if (errorsCount && !prevErrors) prevErrors = errorsCount;\n  }, []);\n\n  // watch changed in cart total items\n  useEffect(() => {\n    if (\n      (totalItems && prevItems !== totalItems) ||\n      (!totalItems && prevItems)\n    ) {\n      // show success\n      showSnackbar(\"SHPORTA U PËRDITËSUA\");\n      prevItems = totalItems;\n    }\n  }, [totalItems]);\n\n  // watch changes in error counter\n  useEffect(() => {\n    if (\n      (errorsCount && prevErrors !== errorsCount) ||\n      (!errorsCount && prevErrors)\n    ) {\n      // show error\n      showSnackbar(\"PATI NJË PROBLEM\");\n    }\n  }, [errorsCount]);\n\n  useEffect(() => {\n    if (snackPack.length && !messageInfo) {\n      // Set a new snack when we don't have an active one\n      setMessageInfo({ ...snackPack[0] });\n      setSnackPack((prev) => prev.slice(1));\n      setOpen(true);\n    } else if (snackPack.length && messageInfo && open) {\n      // Close an active snack when a new one is added\n      setOpen(false);\n    }\n  }, [snackPack, messageInfo, open]);\n\n  const handleClose = (event, reason) => {\n    if (reason === \"clickaway\") {\n      return;\n    }\n    setOpen(false);\n  };\n\n  const handleExited = () => {\n    setMessageInfo(undefined);\n  };\n\n  return (\n    <Snackbar\n      key={messageInfo ? messageInfo.key : undefined}\n      anchorOrigin={{\n        vertical: \"bottom\",\n        horizontal: \"right\",\n      }}\n      TransitionComponent={slide}\n      open={open}\n      autoHideDuration={4000}\n      onClose={handleClose}\n      onExited={handleExited}\n    >\n      <Alert\n        severity=\"success\"\n        variant=\"filled\"\n        style={{ boxShadow: \"0px 5px 10px rgba(0,0,0,0.5)\" }}\n      >\n        {messageInfo && messageInfo.message}\n      </Alert>\n    </Snackbar>\n  );\n};\n\nexport default MySnackbar;\n","/Users/kdingu/Desktop/Pazari-React/pazari/src/components/Products/ProductDrawer/ProductDrawer.jsx",["363","364","365","366"